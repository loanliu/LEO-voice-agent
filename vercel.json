{
  "routes": [
    { "src": "/api/health", "dest": "/api/health" },
    { "src": "/maintenance", "dest": "/api/maintenance" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/src/(.*)", "dest": "/src/$1" },
    { "src": "/public/(.*)", "dest": "/public/$1" },
    { "src": "/(.*)", "dest": "/api/_maintenance-guard" }
  ]
}

